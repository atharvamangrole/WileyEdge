import java.util.Scanner;
import java.util.Random;

public class Hi{
	public static void main(String[] args) {
		//input from user
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter your dog's name: ");
		String userInput = scan.next();
		scan.close();
		System.out.println(userInput+" is: ");
        
        //second time random generated by computer
        Random random1 = new Random();
        int randomNumber1 = random1.nextInt(99);
        Random random2 = new Random();
        int randomNumber2 = random2.nextInt(99);
        Random random3 = new Random();
        int randomNumber3 = random3.nextInt(99);
        Random random4 = new Random();
        int randomNumber4 = random4.nextInt(99);
        Random random5 = new Random();
        int randomNumber5 = random5.nextInt(99);
        
        double one = (randomNumber1+randomNumber2+randomNumber3+randomNumber4+randomNumber5);
        double rn1 = (randomNumber1/one)*100;
        double rn2 = (randomNumber2/one)*100;
        double rn3 = (randomNumber3/one)*100;
        double rn4 = (randomNumber4/one)*100;
        double rn5 = (randomNumber5/one)*100;
        
        
        System.out.println(rn1+"% Labrador ");
        System.out.println(rn2+"% Husky ");
        System.out.println(rn3+"% German Shefered ");
        System.out.println(rn4+"% Poodle ");
        System.out.println(rn5+"% Bulldog ");
		
	}
}
